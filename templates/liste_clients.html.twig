{% extends 'base.html.twig' %}

{% block title %}Liste clients
{% endblock %}

{% block body %}


	<main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
		<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-1 border-bottom">
			<h1 class="h2">Les clients</h1>
			<div class="btn-toolbar mb-2 mb-md-0"></div>
		</div>
<form action="{{ path('app_liste_haie_chercher') }}" method="POST">
<div class="input-group mb-3">
  <input type="text" class="form-control" name="username" placeholder="Chercher les usernames" value="{{ username }}">
</div>
</form>
		<div class="container">
			<div class="row align-items-start">
				{% for client in clients %}
					<div class="col-3 mt-4">
						<!-- Sign up form -->
						<!-- Sign up card -->
							<div class="card person-card"> <div
								class="card-body">
								<!-- Sex image -->
								<img id="img_sex" class="person-img" src="https://visualpharm.com/assets/217/Life%20Cycle-595b40b75ba036ed117d9ef0.svg">
								<h3 id="who_message" class="card-title">{{ client.username|u.truncate(9) }}</h3>
								<!-- First row (on medium screen) -->
							</div>


							<br/>
						</div>
					</div>


				{% endfor %}
			</div>
		</div>

	</div>

</main></div></div>{% endblock %}
